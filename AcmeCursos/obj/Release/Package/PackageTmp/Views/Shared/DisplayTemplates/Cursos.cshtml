@model IEnumerable<AcmeCursos.Models.Curso>
<h3>Curso sLecionados</h3>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Nome)</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Nome)</td>
        </tr>
    }
</table>

